class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        vector<int>haha;
        int j=0;
        for(int i=0;i<nums.size();i++){
            if(val!=nums[i]){
                int k=nums[i];
                haha.push_back(k);
                j++;
            }
        }
        for (int i = 0; i < (int)haha.size(); i++) {
            nums[i] = haha[i];
        }
        return (int)haha.size();
    }
};
