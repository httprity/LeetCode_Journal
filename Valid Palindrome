class Solution {
public:
    bool isPalindrome(string s){
        string s2;
        for(int i=0;i<s.size();i++){
             if ((s[i] >= 'A' && s[i] <= 'Z') || (s[i] >= 'a' && s[i] <= 'z') || (s[i] >= '0' && s[i] <= '9')){
                if(s[i] >= 'A' && s[i] <= 'Z') s[i]+=32;
                s2+=s[i];
            }
        }
        int a=0;
        int b=(int)s2.size()-1;
        while(a<b){
            if(s2[a]!=s2[b]){
                return false;
            }
            else{
                a++;
                b--;
            }

        }
        return true;
    }
};
